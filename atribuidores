#include <stdio.h>

int main () {

    /*
    Atribuição simples =
    Atribuição com soma +=
    Atribuição com subtração -=
    Atribuição com multiplicação *=
    Atribuição com divisão /=
    */

    int numero1, numero2, numero3, resultado;

    resultado = 10;
    printf("Resultado: %d\n, resultado");

    resultado += 20;
    printf("Resultado: %d\n", resultado);


    return 0;
}